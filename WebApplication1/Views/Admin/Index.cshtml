@model IEnumerable<WebApplication1.Models.Admin>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdAdmin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Profit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Salary)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdAdmin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Profit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Salary)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.IdAdmin }) |
            @Html.ActionLink("Details", "Details", new { id = item.IdAdmin }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.IdAdmin })
        </td>
    </tr>
}

</table>

<p>
    @Html.ActionLink("Master Table", "IndexMaster")
</p>

<p>
    @Html.ActionLink("Klient Table", "IndexKlient")
</p>

<p>
    @Html.ActionLink("Car Table", "IndexCar")
</p>